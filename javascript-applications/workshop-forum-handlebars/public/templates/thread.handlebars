{{#each this}}    
    <div class="thread" data-id="{{this.id}}">
      <div><h3><a class="thread-title" href="#/threads/{{this.id}}" id="thread-{{this.id}}">{{this.title}}</a></h3></div>
      <div>
        <p>
          <span>Created on</span><span class="thread-date"></span>
          by <a class="thread-creator" href="#"></a>
        </p>
      </div>
    </div>
{{/each}}